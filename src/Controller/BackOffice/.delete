<?php
declare(strict_types=1);

namespace App\Controller\BackOffice;

use App\View\View;
use App\Model\LogManager;

class LogController
{
    private $logManager;
    private $view;
    private $layout;

    public function __construct(LogManager $logManager, View $view)
    {
        $this->logManager = $logManager;
        $this->view = $view;
        $this->layout = "/wamp64/www/projets/jforteroche/templates/backOffice/layout.html.php";
    }
    public function login() : void
    {
        $data = [];
        $template = "../templates/backoffice/login.html.php";
        $this->view->display($data, $template, $this->layout);
    }
}